{
  "name": "component-template",
  "version": "0.0.1",
  "description": "-业务组件库",
  "main": "lib/index.js",
  "scripts": {
    "lint": "tslint components/**/*.{ts,tsx}",
    "lint:fix": "tslint components/**/*.{ts,tsx} --fix",
    "test": "tslint -c tslint.config ./**/*.ts{,x} & jest test/ --no-cache --json --outputFile=.jest-test-results.json || true",
    "storybook": "start-storybook -p 8004",
    "build-storybook": "build-storybook",
    "build:lib": "rm -rf lib && tsc --emitDeclarationOnly && babel components  --out-dir lib --extensions '.ts,.tsx' --copy-files --config-file './.babelrc-publish'"
  },
  "author": "jiarui.liang@taobao.org",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-Default commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "registry": "https://npm.taobao.org"
  },
  "files": [
    "lib"
  ],
  "lint-staged": {
    "*.{ts,tsx,html,md,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,less}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
